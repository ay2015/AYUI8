                            <Border
                x:Name="AyWindowMovePanel"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                BorderBrush="Transparent"
                BorderThickness="0"
                CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                                <Grid>
                                    <Grid Margin="{Binding TitleBarHeight, RelativeSource={RelativeSource TemplatedParent}, Converter={DoubleToThicknessConverter}, ConverterParameter=1}">
                                        <AdornerDecorator>
                                            <ContentPresenter x:Name="AllCP"/>
                                        </AdornerDecorator>

                                    </Grid>
