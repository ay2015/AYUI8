    <Style x:Key="{x:Type ay:AyWindowSimple}" TargetType="{x:Type ay:AyWindowSimple}">
        <Setter Property="Background"
                Value="{DynamicResource colorwhite}">
        </Setter>
        <Setter Property="AllowsTransparency"
                Value="true"/>
        <Setter Property="WindowStyle"
                Value="None">
        </Setter>
        <Setter Property="BorderThickness"
                Value="1">
        </Setter>
        <Setter Property="BorderBrush"
                Value="{DynamicResource bordercolorbase}">
        </Setter>
        <Setter Property="ay:WindowChrome.WindowChrome">
            <Setter.Value>
                <ay:WindowChrome GlassFrameThickness="0" ResizeBorderThickness="10" CaptionHeight="0"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ay:AyWindowSimple}">
                    <Grid>
                        <Border x:Name="contentBorder" Margin="10" CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" 
                                Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                            <Border.Effect>
                                <DropShadowEffect BlurRadius="6" Opacity="0.1" ShadowDepth="2" Color="Black" />
                            </Border.Effect>
                            <Border.OpacityMask>
                                <VisualBrush>
                                    <VisualBrush.Visual>
                                        <Border Background="Black"
                                        SnapsToDevicePixels="True"
                                        CornerRadius="{Binding CornerRadius, RelativeSource={RelativeSource AncestorType=Border}}"
                                        Width="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType=Border}}"
                                        Height="{Binding ActualHeight, RelativeSource={RelativeSource AncestorType=Border}}" />
                                    </VisualBrush.Visual>
                                </VisualBrush>
                            </Border.OpacityMask>
                            <Grid>
                                <Grid Margin="{TemplateBinding AllContentMargin}">
                                    <AdornerDecorator>
                                        <ContentPresenter x:Name="AllCP"/>
                                    </AdornerDecorator>

                                    <StackPanel Orientation="Horizontal" Margin="{TemplateBinding RightButtonGroupMargin}"
                                            VerticalAlignment="Top" Height="40"
                                            HorizontalAlignment="Right">
                                        <Button VerticalAlignment="Stretch" ay:WindowChrome.IsHitTestVisibleInChrome="True" x:Name="PART_MENU" Width="40"  Style="{StaticResource AYWin_MENU}"   Visibility="{Binding Path=WindowMenuVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"></Button>
                                        <Button  VerticalAlignment="Stretch" ay:WindowChrome.IsHitTestVisibleInChrome="True" x:Name="PART_MIN" Width="40"  Style="{StaticResource AYWin_MIN}"         Visibility="{Binding Path=MinButtonVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"></Button>
                                        <Grid Visibility="{Binding Path=MaxButtonVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                                            <Button   VerticalAlignment="Stretch" ay:WindowChrome.IsHitTestVisibleInChrome="True" x:Name="PART_MAX" Width="40"  Style="{StaticResource AYWin_MAX}"      Visibility="{Binding Path=maxWindowVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"></Button>
                                            <Button  VerticalAlignment="Stretch" ay:WindowChrome.IsHitTestVisibleInChrome="True" x:Name="PART_RESTORE" Width="40"  Style="{StaticResource AYWin_RESTORE}"         Visibility="{Binding Path=restoreWindowVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"></Button>
                                        </Grid>
                                        <Button  VerticalAlignment="Stretch" ay:WindowChrome.IsHitTestVisibleInChrome="True" x:Name="PART_CLOSE" Width="40" Style="{StaticResource AYWin_CLOSE}"         Visibility="{Binding Path=CloseButtonVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"></Button>
                                    </StackPanel>

                                </Grid>
                                <Border CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" >
                                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch"    
                x:Name="AyLayerArea"
        />
                                </Border>
                                <Border CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" >
                                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch"   x:Name="AyLayerAboveArea" >
                                    </Grid>
                                </Border>

      
             
                                <Rectangle  Visibility="Collapsed"  x:Name="AyWindowMaskArea"  RadiusX="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay,Converter={CornerRadiusToRadiusX}}" 
                          RadiusY="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay,Converter={CornerRadiusToRadiusX}}" 
                      Margin="-1" IsHitTestVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                    <Rectangle.Fill>
                                        <SolidColorBrush Color="#00000000"/>
                                    </Rectangle.Fill>
                                </Rectangle>
                            </Grid>


                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsActive" Value="true"></Condition>
                                <Condition Property="WindowState" Value="Normal"></Condition>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="contentBorder" Property="Effect" >
                                <Setter.Value>
                                    <DropShadowEffect BlurRadius="9" Opacity="0.1" ShadowDepth="2" Color="Black" />
                                </Setter.Value>
                            </Setter>
                        </MultiTrigger>
                        <Trigger Property="WindowState" Value="Maximized" >
                            <Setter TargetName="contentBorder" Property="Margin" Value="0" ></Setter>
                            <Setter TargetName="contentBorder" Property="CornerRadius" Value="0" ></Setter>
                            <Setter TargetName="contentBorder" Property="Effect" >
                                <Setter.Value>
                                    <DropShadowEffect BlurRadius="0" ShadowDepth="0" Color="Black"/>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <ControlTemplate x:Key="ayWindowTemplate" TargetType="{x:Type ay:AyWindow}">
        <Grid x:Name="root">
            <Border
                x:Name="AyShadowLayer"
                Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShadowMargin}"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Background="#00FFFFFF"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{Binding Path=ShadowBorderThickness, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">

                <Border.Effect>
                    <DropShadowEffect BlurRadius="{Binding Path=ShadowRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"  Opacity="0.1" ShadowDepth="2" Color="Black" />
                </Border.Effect>
            </Border>

            <!--  方便调整窗体大小  -->
            <Grid
                Name="AyWindowDragBorder"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch" />
            <Border
                x:Name="AyBackgroundBehindLayer"
                 CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShadowMargin}"
                ClipToBounds="True"/>
            <Border
                x:Name="AyBackgroundLayer"
                 CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShadowMargin}"
                Background="{TemplateBinding Background}"
                ClipToBounds="True"
               >
            </Border>

            <Border
                x:Name="AyWindowMovePanel"
                Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShadowMargin, Mode=TwoWay}"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                 BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                <Grid>
                    <Grid Margin="{Binding TitleBarHeight, RelativeSource={RelativeSource TemplatedParent}, Converter={DoubleToThicknessConverter}, ConverterParameter=1}">
                        <AdornerDecorator>
                            <ContentPresenter x:Name="AllCP"/>
                        </AdornerDecorator>
                    </Grid>


                    <DockPanel
                        Name="top"  Background="{Binding Path=WindowTitleBarBg, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                        Height="{Binding TitleBarHeight, RelativeSource={RelativeSource TemplatedParent}}"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Top"
                      
                        LastChildFill="True"
                        Visibility="{Binding Path=WindowRightButtonGroupVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                        <StackPanel
                            Margin="{Binding Path=CloseButtonMargin, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                            DockPanel.Dock="Right" Height="40"
                            Orientation="Horizontal">


                            <Button
                                x:Name="PART_MENU"
                                Width="{Binding Path=RightButtonWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                VerticalAlignment="Stretch"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Center"
                                Focusable="False" 
                                SnapsToDevicePixels="True"
                                Style="{StaticResource AYWin_MENU}"
                                UseLayoutRounding="True"
                                Visibility="{Binding Path=WindowMenuVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                            </Button>
                            <Button
                                x:Name="PART_MIN"
                                Width="{Binding Path=RightButtonWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                VerticalAlignment="Stretch"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Center"
                                Focusable="False"
                                SnapsToDevicePixels="True"
                                Style="{StaticResource AYWin_MIN}"
                                UseLayoutRounding="True"
                                Visibility="{Binding Path=MinButtonVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">

                            </Button>
                            <Grid
                                Width="{Binding Path=RightButtonWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                VerticalAlignment="Stretch"
                                Visibility="{Binding Path=MaxButtonVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                                <Button
                                    x:Name="PART_MAX"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    HorizontalContentAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    Focusable="False"
                                    SnapsToDevicePixels="True"
                                    Style="{StaticResource AYWin_MAX}"
                                    UseLayoutRounding="True"
                                    Visibility="{Binding Path=maxWindowVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">

                                </Button>
                                <Button
                                    x:Name="PART_RESTORE"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    HorizontalContentAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    Focusable="False"
                                    SnapsToDevicePixels="True"
                                    Style="{StaticResource AYWin_RESTORE}"
                                    UseLayoutRounding="True"
                                    Visibility="{Binding Path=restoreWindowVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">

                                </Button>
                            </Grid>

                            <Button
                                x:Name="PART_CLOSE"
                                Width="{Binding Path=RightButtonWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                VerticalAlignment="Stretch"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Center"
                                Focusable="False"
                                SnapsToDevicePixels="True"
                                Style="{StaticResource AYWin_CLOSE}"
                                UseLayoutRounding="True"
                                Visibility="{Binding Path=CloseButtonVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                            </Button>

                        </StackPanel>

                        <ContentPresenter
                            Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ToolBarContent}"
                            ContentTemplate="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ToolBarContentDataTemplate}"
                            DockPanel.Dock="Right" />
                        <StackPanel
                            Margin="5,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Stretch"
                            Orientation="Horizontal"
                            Visibility="{Binding Path=TitleVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                            <Image
                                Width="{Binding Path=WindowIconWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                Height="{Binding Path=WindowIconHeight, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                Source="{TemplateBinding Icon}"
                                Visibility="{Binding Path=WindowIconVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" />
                            <primitive1:AyText
                                Margin="5,0,0,0"
                                VerticalAlignment="Center"        FontSize="{DynamicResource WindowTitleFontSize}"

                                Text="{Binding Path=Title, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" />
                        </StackPanel>
                    </DockPanel>
                </Grid>
            </Border>

            <Border Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShadowMargin, Mode=TwoWay}" CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" >
                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch"    
                x:Name="AyLayerArea"
        />
            </Border>
            <Border Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShadowMargin, Mode=TwoWay}" CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" >
                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch"   x:Name="AyLayerAboveArea" >
                </Grid>
            </Border>
            <Rectangle  Visibility="Collapsed"  x:Name="AyWindowMaskArea"  RadiusX="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay,Converter={CornerRadiusToRadiusX}}" 
                          RadiusY="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay,Converter={CornerRadiusToRadiusX}}" 
                      Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShadowMargin, Mode=TwoWay}" IsHitTestVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <Rectangle.Fill>
                    <SolidColorBrush Color="#00000000"/>
                </Rectangle.Fill>
            </Rectangle>
            <!--  方便调整窗体大小  -->
            <Grid
                Name="AyWindowOuterDragBorder"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch" />
            <!--<ResizeGrip x:Name="WindowResizeGrip" HorizontalAlignment="Right" VerticalAlignment="Bottom" Visibility="Collapsed" IsTabStop="false" />-->
        </Grid>
        <ControlTemplate.Triggers>

            <Trigger Property="WindowEntranceBackgroundMode" Value="1">
                <Setter TargetName="AyShadowLayer" Property="Visibility" Value="Hidden" />
                <Setter TargetName="AyBackgroundLayer" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="top" Property="Visibility" Value="Collapsed" />

            </Trigger>
            <Trigger Property="WindowEntranceBackgroundMode" Value="2">
                <Setter TargetName="AyShadowLayer" Property="Visibility" Value="Hidden" />
            </Trigger>
            <Trigger Property="WindowEntranceBackgroundMode" Value="3">
                <Setter TargetName="AyBackgroundLayer" Property="Visibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="WindowEntranceBackgroundMode" Value="4">
                <Setter TargetName="AyBackgroundLayer" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="top" Property="Visibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="WindowEntranceBackgroundMode" Value="5">
                <Setter TargetName="AyShadowLayer" Property="Visibility" Value="Hidden" />
                <Setter TargetName="AyBackgroundLayer" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="{x:Type ay:AyWindow}" TargetType="{x:Type ay:AyWindow}">
        <Setter Property="Background" Value="{DynamicResource colorwhite}" />
        <Setter Property="WindowTitleBarBg" Value="Transparent" />
        <Setter Property="WindowStyle" Value="None" />
        <Setter Property="AllowsTransparency" Value="True" />
        <Setter Property="ResizeMode" Value="NoResize" />
        <Setter Property="WindowStyle" Value="None" />
        <Setter Property="ResizeMode" Value="NoResize" />
        <Setter Property="BorderBrush"
                Value="{DynamicResource bordercolorbase}">
        </Setter>
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="Template" Value="{StaticResource ayWindowTemplate}" />
