                            <!--  方便调整窗体大小  -->
                            <Border    
                x:Name="AyBackgroundBehindLayer"
                Opacity="{Binding RectangleOpacity3, RelativeSource={RelativeSource AncestorType={x:Type ay:AyWindowShell}, Mode=FindAncestor}}" />
                            <Border  
                x:Name="AyBackgroundLayer"
                Opacity="{Binding RectangleOpacity3, RelativeSource={RelativeSource AncestorType={x:Type ay:AyWindowShell}, Mode=FindAncestor}}">
                                <Grid>
                                    <ay:TransitionPresenter
                        x:Name="tpMainImage"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        Content="{Binding BgData, RelativeSource={RelativeSource AncestorType={x:Type ay:AyWindowShell}, Mode=FindAncestor}}">
                                        <ay:TransitionPresenter.Resources>
                                            <DataTemplate DataType="{x:Type ay:AyTransitionPicture}">
                                                <Rectangle
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    Fill="{Binding ImBrush, Mode=TwoWay}"
                                    RadiusX="{Binding Radius}"
                                    RadiusY="{Binding Radius}"
                                    Stroke="{Binding Stroke}"
                                    StrokeThickness="{Binding StrokeThickness}" />
                                            </DataTemplate>
                                            <DataTemplate DataType="{x:Type ay:AyTransitionColor}">
                                                <Rectangle
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    Fill="{Binding Uri}"
                                    RadiusX="{Binding Radius}"
                                    RadiusY="{Binding Radius}"
                                    Stroke="{Binding Stroke}"
                                    StrokeThickness="{Binding StrokeThickness}" />
                                            </DataTemplate>
                                        </ay:TransitionPresenter.Resources>
                                    </ay:TransitionPresenter>
                                </Grid>
